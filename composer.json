{
  "name": "cat-framework/cat-framework",
  "description": "Cat Framework basado en Clean Architecture con componentes de Laravel.",
  "type": "project",
  "require": {
    "php": ">=7.4",
    "illuminate/database": "^7.0|^8.0|^9.0|^10.0",
    "illuminate/routing": "^7.0|^8.0|^9.0|^10.0",
    "illuminate/http": "^7.0|^8.0|^9.0|^10.0",
    "illuminate/events": "^7.0|^8.0|^9.0|^10.0",
    "illuminate/validation": "^7.0|^8.0|^9.0|^10.0",
    "illuminate/queue": "^7.0|^8.0|^9.0|^10.0",
    "illuminate/cache": "^7.0|^8.0|^9.0|^10.0",
    "illuminate/mail": "^7.0|^8.0|^9.0|^10.0",
    "symfony/console": "^5.0|^6.0",
    "phpoption/phpoption": "^1.9",
    "vlucas/phpdotenv": "^5.0",
    "jenssegers/blade": "^1.4"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Src\\": "src/",
      "App\\Core\\Interfaces\\Controllers\\": "src/Core/Interfaces/Controllers/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "platform": {
      "php": "7.4.0"
    }
  }
}
